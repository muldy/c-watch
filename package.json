{
  "name": "c-watch",
  "author": "MakeAwesomeHappen",
  "version": "1.0.0",
  "keywords": [
    "pebble-app"
  ],
  "private": true,
  "dependencies": {
    "pebble-clay": "^1.0.4"
  },
  "pebble": {
    "displayName": "c-watch",
    "uuid": "c5a3243f-af76-4f22-bbae-35382c1afba3",
    "sdkVersion": "3",
    "enableMultiJS": true,
    "targetPlatforms": [
      "aplite",
      "basalt",
      "chalk",
      "diorite"
    ],
    "watchapp": {
      "watchface": true
    },
    "messageKeys": [
      "TEMPERATURE",
      "CONDITIONS",
      "CLAY_OPENWEATHER_API_KEY",
      "CLAY_CUSTOM_BOTTOM_TEXT",
      "CLAY_VIBRATE_ON_DISCONNECT",
      "CLAY_BACKGROUND_COLLOR"
    ],
    "capabilities": [
      "location",
      "configurable"
    ],
    "resources": {
      "media": [
        {
          "type": "font",
          "name": "FONT_PERFECT_DOS_22",
          "file": "fonts/perfect-dos-vga.ttf",
          "compatibility": "2.7"
        },
        {
          "type": "font",
          "name": "FONT_PERFECT_DOS_48",
          "file": "fonts/perfect-dos-vga.ttf",
          "compatibility": "2.7"
        },
        {
          "type": "font",
          "name": "FONT_PERFECT_DOS_20",
          "file": "fonts/perfect-dos-vga.ttf",
          "compatibility": "2.7"
        },
        {
          "type": "bitmap",
          "name": "IMAGE_BACKGROUND",
          "file": "images/background.png"
        },
        {
          "type": "bitmap",
          "name": "IMAGE_BT_ICON",
          "file": "images/bt-icon.png"
        }
      ]
    }
  }
}
